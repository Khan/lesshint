{
    "parser": "babel-eslint",
    "env": {
        "node": true,
        "mocha": true,
        "es6": true
    },
    "rules": {
        "comma-dangle": [2, "always-multiline"],
        "quotes": [2, "double"],
        "no-unused-vars": 2,
        "space-after-keywords": 2,
        "space-infix-ops": 2,
        "strict": 0
    },
}
